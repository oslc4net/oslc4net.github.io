{
  "$schema": "https://raw.githubusercontent.com/dotnet/docfx/main/schemas/docfx.schema.json",
  "metadata": [
    {
      "src": [
        {
          "src": "../oslc4net/OSLC4Net_SDK/",
          "files": [
            "*/*.csproj"
          ]
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "output": "_site",
    "template": [
      "default",
      "modern",
      "template"
    ],
    "globalMetadata": {
      "_appName": "oslc4net",
      "_appTitle": "oslc4net",
      "_appLogoPath": "images/logo_docfx.png",
      "_appFaviconPath": "images/favicon.ico",
      "_enableSearch": true,
      "pdf": false
    }
  }
}
